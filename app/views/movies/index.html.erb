<h1>Movies</h1>
<%= link_to "New Movie", new_movie_path%>
<br><br>

<%@movies.each do |movie|%>
    <div>
        <%=movie.inspect%>
        <%= link_to "Edit",edit_movie_path(movie)%>
        <%= link_to "Delete Movie", delete_movie_path(movie), method: :delete, data: {confirm: "Are yo usure"}%>
    </div>
<%end%>
